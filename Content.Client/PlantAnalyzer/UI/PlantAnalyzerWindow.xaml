<DefaultWindow xmlns="https://spacestation14.io"
               Title="{Loc plant-analyzer-interface-title}"
               MinSize="300 500"
               SetSize="300 500">
    <BoxContainer
        Name="RootContainer"
        Orientation="Vertical">
        <Label
            Name="PlantName"
            Align="Left" />
        <RichTextLabel
            Name="PlantHP"/>
        <Label
            Name="Diagnostics"
            Text="{Loc plant-analyzer-window-no-seed-information-text}"/>
        <RichTextLabel
            Name="Chemistry"/>
        <RichTextLabel
            Name="Temperature"/>
        <RichTextLabel
            Name="TraitsName"/>
        <Label
            Name="PlantTray"
            Align="Left" />
        <RichTextLabel
            Name="HealthName"/>
        <RichTextLabel
            Name="ProblemsName"/>
    </BoxContainer>
</DefaultWindow>
