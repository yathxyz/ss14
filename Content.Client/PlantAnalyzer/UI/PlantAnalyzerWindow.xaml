<DefaultWindow xmlns="https://spacestation14.io"
               Title="{Loc plant-analyzer-interface-title}"
               MinSize="300 500"
               SetSize="300 500">
    <BoxContainer
        Name="RootContainer"
        Orientation="Vertical">
        <Label
            Name="NoData"
            Text="{Loc plant-analyzer-window-no-seed-information-text}" />
        <BoxContainer
                Name="PlantDataContainer"
                Orientation="Vertical">
            <Label
                Name="PlantName"/>
            <Label
                Name="Yield"/>
            <Label
                Name="Potency"/>
            <Label
                Name="Chemicals"/>
            <Label
                Name="TraitsName"/>
            <Label
                Name="PlantTray" />
            <Label
                Name="HealthName"/>
            <Label
                Name="ProblemsName"/>
        </BoxContainer>
        <BoxContainer
            Name="GroupsContainer"
            Orientation="Vertical">
        </BoxContainer>
    </BoxContainer>
</DefaultWindow>
